{
  "name": "pod-data",
  "description": "Pod Data is an unofficial checklist for tracking your completion progress in NieR: Automata. Mark various items, quests, and achievements as completed, to help keep track of what you've done and what still needs to be accomplished.",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:local": "nuxt dev --dotenv .env.local",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "supabase:types": "supabase gen types typescript --project-id uwxuwdrvfqbgczgpnxwb --schema public > app/types/database.types.ts",
    "supabase:types:local": "supabase gen types typescript --local > app/types/database.types.ts",
    "supabase:dump": "supabase exec db dump -f db_schema.sql"
  },
  "devDependencies": {
    "@nuxthub/core": "0.9.1",
    "@nuxtjs/turnstile": "^1.1.1",
    "nuxt": "4.1.3",
    "nuxt-skew-protection": "0.3.5",
    "prettier": "^3.7.4",
    "sass": "^1.94.0",
    "vuetify-nuxt-module": "^0.18.8"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxtjs/seo": "^3.2.2",
    "@nuxtjs/supabase": "^2.0.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "nuxt-og-image": "^5.1.12",
    "vuetify": "^3.11.2"
  }
}
